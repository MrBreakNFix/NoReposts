{
  "schemaVersion": 1,
  "id": "noreposts",
  "version": "${version}",
  "name": "NoReposts",
  "description": "A configurable library aimed to help keep from illegally reposting your code.",
  "authors": ["MrBreakNFix"],
  "contact": {"email": "mrbreaknfix@gmail.com"},
  "license": "All-Rights-Reserved",
  "icon": "assets/noreposts/icon.png",
  "environment": "client",
  "entrypoints": {
    "preLaunch": [
      "com.mrbreaknfix.noreposts.NoReposts"
    ]
  },
  "mixins": [
    "noreposts.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}"
  }
}
